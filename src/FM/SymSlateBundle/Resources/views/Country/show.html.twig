<h1>Country</h1>

<table class="record_properties">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Iso_code</th>
            <td>{{ entity.isocode }}</td>
        </tr>
        <tr>
            <th>Ps_code</th>
            <td>{{ entity.pscode }}</td>
        </tr>
        <tr>
            <th>Map_code</th>
            <td>{{ entity.mapcode }}</td>
        </tr>
        <tr>
            <th>Name</th>
            <td>{{ entity.name }}</td>
        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('countries') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('countries_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('countries_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
