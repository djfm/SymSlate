{% extends '::base.html.twig' %}



{% block body %}

<h1>User</h1>

<table class="table table-small">
    <tbody>
        <tr>
            <th>Username</th>
            <td>{{ entity.username }}</td>
        </tr>
        
            <th>Email</th>
            <td>{{ entity.email }}</td>
        </tr>
       
        <tr>
            <th>Lastlogin</th>
            <td>{{ entity.lastLogin|date('Y-m-d H:i:s') }}</td>
        </tr>

    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('users') }}">
            Back to the list
        </a>
    </li>
</ul>

{% endblock %}