{% extends '::base.html.twig' %}

{% block body %}
<h1>MessagesImport list</h1>

<table class="table table-striped">
    <thead>
        <tr>
            <th>Id</th>
            <th>Original_filename</th>
            <th>Filepath</th>
            <th>Pack_id</th>
            <th>Created</th>
            <th>Updated</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
    {% for entity in entities %}
        <tr>
            <td><a href="{{ path('messagesimports_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
            <td>{{ entity.originalfilename }}</td>
            <td>{{ entity.filepath }}</td>
            <td>{{ entity.packid }}</td>
            <td>{% if entity.created %}{{ entity.created|date('Y-m-d H:i:s') }}{% endif %}</td>
            <td>{% if entity.updated %}{{ entity.updated|date('Y-m-d H:i:s') }}{% endif %}</td>
            <td>
                
                        <a class="btn btn-success" href="{{ path('messagesimports_show', { 'id': entity.id }) }}">show</a>
                   
                        <a class="btn btn-warning" href="{{ path('messagesimports_edit', { 'id': entity.id }) }}">edit</a>
                    
                
            </td>
        </tr>
    {% endfor %}
    </tbody>
</table>

<ul>
    <li>
        <a href="{{ path('messagesimports_new') }}">
            Create a new entry
        </a>
    </li>
</ul>

{% endblock %}