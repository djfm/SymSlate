{% extends '::base.html.twig' %}



{% block body %}

<h1>CountryLanguage edit</h1>



<form action="{{ path('countrylanguage_edit', { 'id': entity.id }) }}" method="post">
    {{ form_widget(edit_form) }}
    <button class="btn btn-warning input-small" type="submit">Edit</button>
</form>
<br/>
<form action="{{ path('countrylanguage_delete', { 'id': entity.id }) }}" method="post">
    <button class="btn btn-danger input-small" type="submit">Delete</button>
</form>
<br/>
<a href="{{ path('countrylanguage') }}">
    &lt;&lt; Back to the list
</a>

{% endblock %}