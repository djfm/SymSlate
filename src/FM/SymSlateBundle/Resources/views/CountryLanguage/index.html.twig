{% extends '::base.html.twig' %}



{% block body %}

<h1>CountryLanguage list</h1>

<table class="records_list">
    <thead>
        <tr>
            <th>Id</th>
            <th>Country_id</th>
            <th>Language_id</th>
            <th>Percent</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
    {% for entity in entities %}
        <tr>
            <td><a href="{{ path('countrylanguage_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
            <td>{{ entity.countryid }}</td>
            <td>{{ entity.languageid }}</td>
            <td>{{ entity.percent }}</td>
            <td>
                <ul>
                    <li>
                        <a href="{{ path('countrylanguage_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('countrylanguage_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
            </td>
        </tr>
    {% endfor %}
    </tbody>
</table>

<ul>
    <li>
        <a href="{{ path('countrylanguage_new') }}">
            Create a new entry
        </a>
    </li>
</ul>

{% endblock %}