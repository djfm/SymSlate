<h1>TranslationSubmission list</h1>

<table class="records_list">
    <thead>
        <tr>
            <th>Id</th>
            <th>User</th>
            <th>Created</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
    {% for entity in entities %}
        <tr>
            <td><a href="{{ path('translationsubmission_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
            <td>{{ entity.getUser().getUsername() }}</td>
            <td>{% if entity.created %}{{ entity.created|date('Y-m-d H:i:s') }}{% endif %}</td>
            <td>
                <ul>
                    <li>
                        <a href="{{ path('translationsubmission_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('translationsubmission_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
            </td>
        </tr>
    {% endfor %}
    </tbody>
</table>
